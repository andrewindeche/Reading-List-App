{
  "name": "reading-list-app",
  "version": "1.0.0",
  "engine": {
    "version": "v21.4.0"
  },
  "description": "A teacher Facing UI for assigning books to students",
  "main": "./src/index.js",
  "scripts": {
    "start": "webpack serve --open --port 3000 --config webpack.prod.js",
    "lint": "eslint",
    "build": "webpack --config webpack.prod.js",
    "test": "jest",
    "clean": "rm -rf dist/",
    "dev": "webpack serve --open --port 3000 --config webpack.dev.js"
  },
  "jest": {
    "verbose": true,
    "collectCoverage": true,
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "html",
      "text"
    ],
    "testEnvironment": "jsdom",
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleNameMapper": {
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "transform": {
      "^.+\\.jsx?$": "babel-jest",
      "^.+\\.(ico|svg|jpeg|jpg|png|gif|webp)$": "jest-transform-stub"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andrewindechemain/Reading-List-App.git"
  },
  "keywords": [
    "Reading-List",
    "React",
    "Ello"
  ],
  "author": "Andrew Indeche",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/andrewindechemain/Reading-List-App/issues"
  },
  "homepage": "https://github.com/andrewindechemain/Reading-List-App#readme",
  "dependencies": {
    "@apollo/client": "^3.10.4",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@mui/icons-material": "^5.15.19",
    "@mui/material": "^5.15.19",
    "axios": "^1.7.2",
    "babel-loader": "^9.1.3",
    "babel-preset": "^1.1.7",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^7.1.2",
    "env": "^0.0.2",
    "file-loader": "^6.2.0",
    "graphql": "^16.8.1",
    "html-webpack-plugin": "^5.6.0",
    "identity-obj-proxy": "^3.0.0",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.23.1",
    "style-loader": "^4.0.0",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.7",
    "@babel/core": "^7.24.7",
    "@babel/preset-env": "^7.24.7",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.15",
    "@testing-library/jest-dom": "^6.4.5",
    "@testing-library/react": "^16.0.0",
    "@typescript-eslint/eslint-plugin": "^7.12.0",
    "@typescript-eslint/parser": "^7.12.0",
    "compression-webpack-plugin": "^11.1.0",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^28.6.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.34.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-testing-library": "^6.2.2",
    "jest-cli": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-fetch-mock": "^3.0.3",
    "jest-junit": "^16.0.0",
    "jest-transform-stub": "^2.0.0",
    "react-refresh": "^0.14.2",
    "react-test-renderer": "^18.3.1",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-merge": "^5.10.0"
  }
}
